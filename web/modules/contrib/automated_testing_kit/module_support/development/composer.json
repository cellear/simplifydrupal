    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/coffee": "^1.0",
        "drupal/core": "~10.1.0",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/ctools": "~4",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "~5.0",
        "drupal/devel_kint_extras": "~1.1",
        "drupal/gin": "^3.0@RC",
        "drupal/pathauto": "~1.0",
        "drupal/qa_accounts": "^1",
        "drupal/redirect": "^1.8",
        "drupal/simple_sitemap": "~4.0",
        "drupal/symfony_mailer": "*",
        "drupal/token": "~1",
        "drupal/upgrade_status": "^4.0",
        "drupal/webform": "~6",
        "drupal/xmlsitemap": "^1.5",
        "drush/drush": "^12.2",
        "progress-tracker/progress-tracker": "*",
        "signature_pad/signature_pad": "*",
        "symfony/amazon-mailer": "^6.2",
        "tabby/tabby": "*",
        "tippyjs/6.x": "*",
        "twig/extra-bundle": "~3.7"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true
        }
    },
    "scripts": {
        "cypress:install": [
            "project/scripts/cypress/cypress-install"
        ],
        "cypress:export-translations": [
            "drush scr project/scripts/cypress/export-translations.php"
        ]

    },
    "extra": {
        "enable-patching": true,
        "patches": {
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
